#include<iostream>
using namespace std;

int main()
{
    int a,b,c,d;
    for(a=1;a<10;a++)
        for(b=0;b<10;b++)
            for(c=0;c<10;c++)
                for(d=0;d<10;d++)
                    if((a*1000+b*100+c*10+d)*9==(d*1000+c*100+b*10+a))
                        cout<<a<<b<<c<<d<<endl;
}
